{
  "categories": [
    {
      "title": "### Breaking Changes",
      "rules": [
        {
          "pattern": "^(break|BREAKING)",
          "on_property": "title"
        }
      ]
    },
    {
      "title": "### Features",
      "rules": [
        {
          "pattern": "^(feat|Add|Support|Introduce)",
          "on_property": "title"
        }
      ]
    },
    {
      "title": "### Bug Fixes",
      "rules": [
        {
          "pattern": "^(fix|Fix|Restore)",
          "on_property": "title"
        }
      ]
    },
    {
      "title": "### Performance",
      "rules": [
        {
          "pattern": "(perf|Refactor fee|Deterministic|optimiz)",
          "on_property": "title"
        }
      ]
    },
    {
      "title": "### Documentation",
      "rules": [
        {
          "pattern": "(doc|Doc|Update docs|README)",
          "on_property": "title"
        }
      ]
    },
    {
      "title": "### Dependencies",
      "rules": [
        {
          "pattern": "(Bump|deps|dependencies|Update package)",
          "on_property": "title"
        }
      ]
    }
  ],
  "sort": {
    "order": "ASC",
    "on_property": "authoredDate"
  },
  "template": "#{{CHANGELOG}}\n\n### Other Changes\n#{{UNCATEGORIZED}}",
  "commit_template": "- #{{TITLE}} (#{{SHORT_SHA}})",
  "empty_template": "- No changes",
  "max_tags_to_fetch": 200,
  "max_back_track_time_days": 365,
  "tag_resolver": {
    "method": "semver"
  },
  "custom_placeholders": [
    {
      "name": "SHORT_SHA",
      "source": "MERGE_SHA",
      "transformer": {
        "pattern": "^(.{7}).*$",
        "target": "$1"
      }
    }
  ]
}
